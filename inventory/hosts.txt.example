[vars]
#ssh_public_key_path=~/.ssh/id_ed25519.pub
#rke2_version=v1.31.4+rke2r1

# Six Node Cluster
[six_node]
k1 192.168.0.11
k2 192.168.0.12
k3 192.168.0.13
l3 192.168.1.3
l4 192.168.1.4 agent_mount_device=/dev/sda1
l5 192.168.1.20 agent_mount_device=/dev/sda1
node6 192.168.0.16
node7 192.168.0.17
node8 192.168.0.18

[control_plane_nodes]
k1
k2
k3

[worker_nodes]
l3
l4
l5
node6
node7
node8
